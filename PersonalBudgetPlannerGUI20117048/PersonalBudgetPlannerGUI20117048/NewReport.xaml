            <TextBox x:Name="vehicleTDText" HorizontalAlignment="Left" Margin="218,104,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="vehicleTDText_TextChanged" Background="#FFEEEEEE" TabIndex="21"/>
